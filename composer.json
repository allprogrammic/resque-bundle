{
  "name": "allprogrammic/resque-bundle",
  "type": "symfony-bundle",
  "description": "Symfony ResqueBundle",
  "keywords": ["Asynchronous task management"],
  "homepage": "http://github.com/allprogrammic/resque-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "William Pottier",
      "email": "developer@william-pottier.fr"
    },
    {
      "name": "AllProgrammic",
      "homepage": "https://allprogrammic.com"
    }
  ],
  "require": {
    "php": "^5.5.9 || ^7.0 || ^8.0",
    "ext-redis": "*",
    "allprogrammic/redis-client": "1.0.x-dev",
    "allprogrammic/resque": "1.0.x-dev",
    "symfony/framework-bundle": "^2.7 || ^3.0 || ^4.1 || 5.0",
    "symfony/twig-bundle": "^2.7 || ^3.0 || ^4.1 || 5.0",
    "twig/twig": "^1.28 || ^2.0",
    "symfony/templating": "^2.7 || ^3.0 || ^4.1 || 5.0",
    "symfony/swiftmailer-bundle": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "AllProgrammic\\Bundle\\ResqueBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "support": {
    "docs": "https://github.com/AllProgrammic/resque-bundle/wiki"
  }
}
