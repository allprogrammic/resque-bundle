{% extends '@AllProgrammicResque/layout.html.twig' %}

{% block link_stats_attr %} class="active"{% endblock %}

{% block content %}
    {{ render(controller('AllProgrammicResqueBundle:Chart:index')) }}

    <h1 class="wi">Resque</h1>
    {{ render(controller('AllProgrammicResqueBundle:Stats:info')) }}

    <h1 class="wi">Redis</h1>
    <table class="table table-striped stats">
        {% for key, value in infos %}
            <tr>
                <th>{{ key }}</th>
                <td>{{ value }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}